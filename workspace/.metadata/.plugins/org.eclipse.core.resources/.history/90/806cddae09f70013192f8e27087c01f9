#include <iostream>
#include <cstdio>
#include <cstdlib>
#include <cstring>

const char Goal[] = "Begin the Escape execution at the Break of Dawn";

char s[100];
int ans;
bool flag;

void work(int a, int b, int c) {
//	puts(s);
	static char tmp[100];
	int len = strlen(s), tot = 0;
	for(int i = 0; i < a; ++i) {
		tmp[tot++] = s[i];
	}
	for(int i = b + 1; i < c; ++i) {
		tmp[tot++] = s[i];
	}
	for(int i = a + 1; i < b; ++i) {
		tmp[tot++] = s[i];
	}
	for(int i = c + 1; i < len; ++i) {
		tmp[tot++] = s[i];
	}
	tmp[tot] = 0;
	strcpy(s, tmp);
//	puts(tmp);
}

void dfs(int depth) {
	if(strcmp(s, Goal) == 0) {
		ans = depth;
		flag = true;
		return;
	}
	char pre[100];
	static char sub[100];
	strcpy(pre, s);
	int len = strlen(s);
	for(int i = 0, j = 0; i < len; ++i) {
		if(s[i] == 'C' || s[i] == 'O' || s[i] == 'W') {
			sub[j] = 0;
			puts(sub);
			puts(Goal);
			if(!strstr(Goal, sub)) {
				return;
			}
			j = 0;
		}
		else {
			sub[j++] = s[i];
		}
	}
	for(int j = 0; j < len; ++j) {
		if(s[j] == 'O') {
			for(int k = len - 1; k > j; --k) {
				if(s[k] == 'W') {
					for(int i = 0; i < j; ++i) {
						if(s[i] == 'C') {
//							printf("%d %d %d\n", i, j, k);
//							puts(sub);
//							puts(Goal);
//							std::cout << (void*)strstr(Goal, sub) << std::endl;
							if(!strstr(Goal, sub)) {
								continue;
							}
							work(i, j, k);
							dfs(depth + 1);
							if(flag) {
								return;
							}
							strcpy(s, pre);
						}
					}
				}
			}
		}
	}
}

int main() {
	freopen("cryptcow.in", "r", stdin);
	freopen("cryptcow.out", "w", stdout);
	gets(s);
	dfs(0);
	printf("%d %d\n", flag ? 1 : 0, ans);
	fprintf(stderr, "done\n");
	return 0;
}
