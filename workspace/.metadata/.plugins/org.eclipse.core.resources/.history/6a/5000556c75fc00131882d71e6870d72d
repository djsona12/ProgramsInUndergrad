import java.util.Arrays;
import java.util.Scanner;

public class Main {
	Scanner input = new Scanner(System.in);

	final int dx[] = { -1, 0, 0, 1 };
	final int dy[] = { 0, 1, -1, 0 };

	int N, M, g[][][], dis[][];
	Grid q[];

	void run() {
		for (;;) {
			N = input.nextInt();
			M = input.nextInt();
			if(N == 0 && M == 0) {
				return;
			}
			g = new int[4][N][M];
			dis = new int[N][M];
			q = new Grid[N * M];
			for (int i = 0; i < N; ++i) {
				for (int j = 1; j < M; ++j) {
					g[1][i][j - 1] = g[2][i][j] = input.nextInt();
				}
				if (i == 0) {
					for (int j = 0; j < M; ++j) {
						g[0][i][j] = 1;
					}
				} else {
					if (i == N - 1) {
						for (int j = 0; j < M; ++j) {
							g[3][i][j] = 1;
						}
					} else {
						for (int j = 0; j < M; ++j) {
							g[0][i][j] = g[3][i - 1][j] = input.nextInt();
						}
					}
				}
			}
		}
	}

	public static void main(String args[]) {
		new Main().run();
	}
}

class Grid {
	int x, y;

	Grid(int x, int y) {
		this.x = x;
		this.y = y;
	}
}
